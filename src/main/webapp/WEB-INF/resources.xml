<resources>
    
    
    <Resource id="TravelcAsynchronousPool" type="ManagedExecutorService">
        Core 100
        Max 700
        Queue 150000
        KeepAlive 1 s
        ThreadFactory TravelcAsyncThreadFactory
        Lazy true
    </Resource>     

    <Resource id="TravelcAsyncThreadFactory" type="ManagedThreadFactory">
        Prefix = tr2-async-
        Lazy = true
    </Resource>        
    
    <Container id="myStatelessContainer" type="STATELESS">
        strictPooling = false
    </Container>
    
</resources>